package org.wildstang.year2021.auto.steps;

import org.wildstang.framework.auto.steps.AutoStep;
import org.wildstang.year2021.subsystems.arm.Arm;

public class ArmDownwardsStep extends AutoStep {
    Arm arm = new Arm();

    public driveDownwardsStep() {
        this(1.0);
    }

    public driveDownwardsStep(double: distance) {
    moveArm(distance, false);
    }

    public void initialize() {
    moveArm(double, boolean);
    }

    public void update() {
        if (Arm.arm.armMoving == false) {
        setFinished(true);
        }
    }

    public String toString() {
        return "ArmDownwardsStep";
    }
}